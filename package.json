{
  "name": "bgagel",
  "version": "0.0.1",
  "description": "Busines.gov.au GEL components",
  "main": "dist/bgagel.cjs.js",
  "jsnext:main": "dist/bgagel.es.js",
  "module": "dist/bgagel.es.js",
  "scripts": {
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "test": "cross-env BABEL_ENV=test react-scripts test --env=jsdom",
    "cover": "npm test -- --coverage",
    "start": "cross-env BABEL_ENV=webpack webpack-dev-server --config ./webpack.fed.config.js --watch",
    "build-docs": "cross-env BABEL_ENV=webpack WEBPACK_BUILD=production webpack --config ./webpack.fed.config.js --progress --colors",
    "build": "rollup -c",
    "prebuild": "cross-env BABEL_ENV=lib-dir babel src --out-dir lib --ignore src/__tests__/",
    "postbuild": "node ./scripts/postbuild.js",
    "create-release": "npm test && sh ./scripts/release",
    "publish-release": "npm test && sh ./scripts/publish",
    "lint": "eslint src"
  },
  "browserslist": [
    "last 2 versions",
    "safari >= 10",
    "ie >= 11",
    "not ie <= 10"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bgagel/bgagel.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib",
    "dist"
  ],
  "contributors": [
    "Alec Pirillo <alec.pirillo@hosthavas.com>"
  ],
  "dependencies": {
    "classnames": "^2.2.3",
    "core-js": "^2.5.3",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2",
    "lodash.tonumber": "^4.0.3",
    "prop-types": "^15.5.8",
    "react-popper": "0.8.1",
    "react-portal": "^4.1.2",
    "react-transition-group": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "autoprefixer": "^7.2.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-remove-webpack": "^1.1.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react-app": "^3.1.1",
    "bootstrap": "^4.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "conventional-changelog-cli": "^1.3.15",
    "conventional-recommended-bump": "^2.0.6",
    "copy-webpack-plugin": "^4.4.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.1",
    "ejs": "^2.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "holderjs": "^2.9.4",
    "json-loader": "^0.5.7",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "reactstrap": "^5.0.0-alpha.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.0.3",
    "react-prism": "^4.3.2",
    "react-router": "^3.2.0",
    "react-scripts": "^1.1.1",
    "react-test-renderer": "^16.0.0",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-babel-minify": "^4.0.0",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.20.2",
    "webpack": "^3.8",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}
